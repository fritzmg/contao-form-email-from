{
	"name":"fritzmg/contao-form-email-from",
	"description":"Mini Contao extension to automatically set a reply address in contact forms.",
	"keywords":["contao", "form", "email"],
	"type":"contao-module",
	"homepage":"http://www.inspiredminds.at",
	"license":"GPL-2.0",
	"authors":[
		{
			"name":"Fritz Michael Gschwantner",
			"homepage":"http://www.inspiredminds.at",
			"email":"fmg@inspiredminds.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"fmg@inspiredminds.at",
		"issues":"https://github.com/fritzmg/contao-form-email-from/issues",
		"source":"https://github.com/fritzmg/contao-form-email-from",
		"forum":"https://community.contao.org/de"
	},
	"require":{
		"php":">=5.2",
		"contao/core":">=3.0,<3.6",
		"contao-community-alliance/composer-installer":"*"
	},
    "replace": {
        "contao-legacy/form_email_from": "self.version"
    },
	"extra":{
		"contao":{
			"sources":{
				"system/modules/form_email_from":"system/modules/form_email_from"
			}
		}
	}
}
